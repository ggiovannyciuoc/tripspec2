    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.0.0",
    "parcel-plugin-imagemin": "^4.0.2",
    "postcss": "^8.3.11",
    "rimraf": "^3.0.2"
  }
}
